<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module rename-to='apis_maps_test'>

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. -->

	<!-- I'm using JSON for debugging -->
	<inherits name="com.google.gwt.json.JSON" />

	<!-- Other module inherits -->
	<inherits name='com.google.gwt.maps.Apis_Google_Maps' />

	<!-- Specify the app entry point class. -->
	<entry-point class='com.google.gwt.maps.testing.client.Apis_Maps_Test' />

	<!-- Style sheets -->
	<stylesheet src="css/style.css" />

	<!-- Specify the paths for translatable code -->
	<source path='client' />

</module>